@font-face {
  font-family: 'chat-font-icon';
  src:  url('./common/fonts/chat-font-icon.eot?dmtnku');
  src:  url('./common/fonts/chat-font-icon.eot?dmtnku#iefix') format('embedded-opentype'),
    url('./common/fonts/chat-font-icon.ttf?dmtnku') format('truetype'),
    url('./common/fonts/chat-font-icon.woff?dmtnku') format('woff'),
    url('./common/fonts/chat-font-icon.svg?dmtnku#chat-font-icon') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'chat-font-icon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

